{
    "configurations": [
        {
            "name": "Linux-Python-Bindings",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../cpp-core/include/**",
                "/usr/include",
                "/usr/local/include",
                "${HOME}/.pyenv/versions/**/include/python*"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/../../build/python-bindings/compile_commands.json"
        }
    ],
    "version": 4
}